    <BitmapImage x:Key="ImgAdd" UriSource="/Images/Themes/Dark/add.png" />
    <BitmapImage x:Key="ImgDelete" UriSource="/Images/Themes/Dark/delete.png" />

    <BitmapImage x:Key="ImgPlay64" UriSource="/Images/Themes/Dark/play_64px.png" />
    <BitmapImage x:Key="ImgPrevious64" UriSource="/Images/Themes/Dark/previous_64px.png" />

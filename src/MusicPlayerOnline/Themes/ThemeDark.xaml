<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="Top" Color="#2D2D30"></SolidColorBrush>
    <SolidColorBrush x:Key="Body" Color="#222222"></SolidColorBrush>
    <SolidColorBrush x:Key="Bottom" Color="#333333"></SolidColorBrush>
    <SolidColorBrush x:Key="FontColor" Color="#E6E6E6"></SolidColorBrush>
    <SolidColorBrush x:Key="TopBorder" Color="#E6E6E6"></SolidColorBrush>
    <SolidColorBrush x:Key="BodyBorder" Color="#3F3F3F"></SolidColorBrush>

    <SolidColorBrush x:Key="MusicSearchResultBackground" Color="#222222"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultAlternation" Color="#2E2E2E"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultSource" Color="#F6F6F6"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultName" Color="#E6E6E6"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultOther" Color="#959595"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultMouseOver" Color="#3E3E3E"></SolidColorBrush>
    <SolidColorBrush x:Key="MusicSearchResultSelected" Color="#2D2D2D"></SolidColorBrush>


    <SolidColorBrush x:Key="PlaylistBorder" Color="#3E3E3E"></SolidColorBrush>
    <SolidColorBrush x:Key="PlaylistBackground" Color="#222222"></SolidColorBrush>
    <SolidColorBrush x:Key="PlaylistName" Color="#E6E6E6"></SolidColorBrush>
    <SolidColorBrush x:Key="PlaylistPlaying" Color="#F38B76"></SolidColorBrush>
    <SolidColorBrush x:Key="PlaylistHeadFore" Color="#7AC1FF"></SolidColorBrush>
    

    <BitmapImage x:Key="ImgSettings" UriSource="/Images/Themes/Dark/settings.png" />
